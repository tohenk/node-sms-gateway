<% script.create('jQuery')
  .useDependencies(['jQuery/Util', 'SemanticUI/Dialog'])
  .addMiddle(`
$.errhandler.register(401, function() {
    var dlgId = 'err401';
    if ($.ntdlg.isVisible(dlgId)) return;
    $.ntdlg.dialog(dlgId, 'Information', 'Your session has been expired, please try to login again!', $.ntdlg.ICON_ALERT, {
        'okay': {
            type: 'green approve',
            caption: '<i class="check icon"></i>Ok',
            handler: function() {
                window.location.reload();
            }
        }
    });
});
`); %>